{
  "form-config": {
    "title": "Lead Information",
    "description": "Enter the details for this lead contact",
    "fields": [
      {
        "name": "image",
        "label": "Lead Image",
        "type": "image",
        "required": false,
        "placeholder": "Upload or paste image URL"
      },
      {
        "name": "name",
        "label": "Full Name",
        "type": "text",
        "required": true,
        "placeholder": "Enter full name",
        "validation": {
          "minLength": 2,
          "maxLength": 100,
          "message": "Name must be between 2 and 100 characters"
        }
      },
      {
        "name": "email",
        "label": "Email Address",
        "type": "email",
        "required": true,
        "placeholder": "example@company.com",
        "validation": {
          "pattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$",
          "message": "Please enter a valid email address"
        }
      },
      {
        "name": "company",
        "label": "Company",
        "type": "text",
        "required": false,
        "placeholder": "Enter company name",
        "validation": {
          "maxLength": 150,
          "message": "Company name cannot exceed 150 characters"
        }
      },
      {
        "name": "title",
        "label": "Job Title",
        "type": "text",
        "required": false,
        "placeholder": "Enter job title",
        "validation": {
          "maxLength": 100,
          "message": "Job title cannot exceed 100 characters"
        }
      },
      {
        "name": "phone",
        "label": "Phone Number",
        "type": "tel",
        "required": false,
        "placeholder": "+1 (555) 123-4567",
        "validation": {
          "pattern": "^[\\+]?[\\d\\s\\(\\)\\-\\.]+$",
          "message": "Please enter a valid phone number"
        }
      },
      {
        "name": "tags",
        "label": "Tags",
        "type": "tags",
        "required": false,
        "placeholder": "Add tags (e.g., enterprise, high-priority, referral)",
        "validation": {
          "message": "Tags help organize and filter leads"
        }
      },
      {
        "name": "notes",
        "label": "Notes",
        "type": "textarea",
        "required": false,
        "placeholder": "Add any additional notes about this lead...",
        "multiline": true,
        "rows": 4,
        "validation": {
          "maxLength": 2000,
          "message": "Notes cannot exceed 2000 characters"
        }
      }
    ],
    "submitLabel": "Save Lead",
    "cancelLabel": "Cancel"
  },
